    const TONE_SAMPLE_POINTS = [
        [0.5, 0.5],
        [0.1, 0.1],
        [0.9, 0.1],
        [0.1, 0.9],
        [0.9, 0.9],
        [0.5, 0.15],
        [0.5, 0.85],
        [0.15, 0.5],
        [0.85, 0.5],
        [0.3, 0.3],
        [0.7, 0.3],
        [0.3, 0.7],
        [0.7, 0.7],
        [0.2, 0.2],
        [0.8, 0.2],
        [0.2, 0.8],
        [0.8, 0.8],
    ];

    function safeGetValue(key, fallback) {
        try {
            if (typeof GM_getValue === 'function') {
                return GM_getValue(key, fallback);
            }
        } catch (_) {}
        try {
            const raw = localStorage.getItem(key);
            if (raw == null) return fallback;
            if (raw === 'true') return true;
            if (raw === 'false') return false;
            return raw;
        } catch (_) {
            return fallback;
        }
    }

    function safeSetValue(key, value) {
        try {
            if (typeof GM_setValue === 'function') {
                GM_setValue(key, value);
                return;
            }
        } catch (_) {}
        try {
            localStorage.setItem(key, String(value));
        } catch (_) {}
    }

    function hostKey() {
        return `${MODE_PREF_PREFIX}${location.hostname.toLowerCase()}`;
    }

    function getPreference() {
        if (cachedPreference == null) cachedPreference = safeGetValue(HOST_PREF_KEY, "auto");
        return cachedPreference;
    }

    function parseRgb(text) {
        if (!text) return null;
        const hex = text.trim().match(/^#([0-9a-f]{3,8})$/i);
        if (hex) {
            const v = hex[1];
            if (v.length === 3 || v.length === 4) {
                return [
                    parseInt(v[0] + v[0], 16),
                    parseInt(v[1] + v[1], 16),
                    parseInt(v[2] + v[2], 16),
                ];
            }
            if (v.length >= 6) {
                return [
                    parseInt(v.slice(0, 2), 16),
                    parseInt(v.slice(2, 4), 16),
                    parseInt(v.slice(4, 6), 16),
                ];
            }
        }
        const m = text.match(/rgba?\(([^)]+)\)/i);
        if (!m) return null;
        const nums = m[1].split(",").map((v) => Number.parseFloat(v.trim()));
        if (nums.length < 3 || nums.slice(0, 3).some((n) => Number.isNaN(n))) return null;
        return [nums[0], nums[1], nums[2]];
    }

    function luminance(rgb) {
        const s = rgb.map((v) => Math.max(0, Math.min(255, v)) / 255).map((c) => (c <= 0.03928 ? c / 12.92 : ((c + 0.055) / 1.055) ** 2.4));
        return 0.2126 * s[0] + 0.7152 * s[1] + 0.0722 * s[2];
    }

    function contrast(l1, l2) {
        const hi = Math.max(l1, l2);
        const lo = Math.min(l1, l2);
        return (hi + 0.05) / (lo + 0.05);
    }

    function effectiveColor(element, prop) {
        let node = element;
        while (node && node.nodeType === Node.ELEMENT_NODE) {
            const value = getComputedStyle(node)[prop];
            const rgb = parseRgb(value);
            if (rgb) {
                const alphaMatch = value.match(/rgba\([^,]+,[^,]+,[^,]+,\s*([\d.]+)\)/i);
                const alpha = alphaMatch ? Number.parseFloat(alphaMatch[1]) : 1;
                if (!Number.isNaN(alpha) && alpha > 0.025) return rgb;
            }
            node = node.parentElement;
        }
        return null;
    }

    function hasStrongDarkSignals() {
        const metaColorScheme = document.querySelector('meta[name="color-scheme"]');
        if (metaColorScheme) {
            const c = String(metaColorScheme.content || "").toLowerCase();
            if (c.includes("dark") && !c.includes("only light")) return true;
        }
        const htmlScheme = String(getComputedStyle(HTML).colorScheme || "").toLowerCase();
        const bodyScheme = document.body ? String(getComputedStyle(document.body).colorScheme || "").toLowerCase() : "";
        if (htmlScheme.includes("dark") || bodyScheme.includes("dark")) return true;
        const themeColorMeta = document.querySelector('meta[name="theme-color"]');
        if (themeColorMeta) {
            const rgb = parseRgb(themeColorMeta.content || "");
            if (rgb && luminance(rgb) < 0.42) return true;
        }
        return false;
    }

    function toneSample() {
        const w = Math.max(window.innerWidth || 1, 1);
        const h = Math.max(window.innerHeight || 1, 1);
        const htmlBg = parseRgb(getComputedStyle(HTML).backgroundColor);
        let darkVotes = 0;
        let lightVotes = 0;
        let votes = 0;
        for (const [xr, yr] of TONE_SAMPLE_POINTS) {
            const x = Math.max(0, Math.min(w - 1, Math.floor(w * xr)));
            const y = Math.max(0, Math.min(h - 1, Math.floor(h * yr)));
            const element = document.elementFromPoint(x, y);
            if (!element) continue;
            const bg = effectiveColor(element, "backgroundColor") || htmlBg;
            const fg = effectiveColor(element, "color") || parseRgb(getComputedStyle(element).color);
            if (!bg || !fg) continue;
            const bgLum = luminance(bg);
            const fgLum = luminance(fg);
            const cr = contrast(bgLum, fgLum);
            votes += 1;
            if (bgLum < 0.43 && fgLum > bgLum && cr >= 2.0) darkVotes += 1;
            if (bgLum > 0.62 && cr >= 2.0) lightVotes += 1;
        }
        if (!votes) return null;
        return { darkRatio: darkVotes / votes, lightRatio: lightVotes / votes, votes };
    }

    function detectMode() {
        const pref = getPreference();
        if (pref === "enhance" || pref === "invert") return pref;
        const host = location.hostname.toLowerCase();
        if (KNOWN_DARK_HOSTS.has(host)) return "enhance";
        if (hasStrongDarkSignals()) return "enhance";
        const sample = toneSample();
        if (sample) {
            if (sample.darkRatio >= 0.55) return "enhance";
            if (sample.lightRatio >= 0.55) return "invert";
            return sample.darkRatio >= sample.lightRatio ? "enhance" : "invert";
        }
        const htmlBg = parseRgb(getComputedStyle(HTML).backgroundColor);
        const bodyBg = document.body ? parseRgb(getComputedStyle(document.body).backgroundColor) : null;
        const bg = bodyBg || htmlBg;
        if (!bg) return "enhance";
        return luminance(bg) < 0.5 ? "enhance" : "invert";
    }

    function selectorsToCss(selectors, declarations) {
        let css = "";
        for (const selector of selectors) {
            css += `html.vm-total-dark[${MODE_ATTR}="enhance"] ${selector} { ${declarations} }
`;
        }
        return css;
    }

