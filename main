    function ensureStyle() {
        if (styleTagAdded) return;
        GM_addStyle(buildStyleCss());
        styleTagAdded = true;
    }

    function updateButtonLabel() {
        if (!toggleButton) return;
        const enabled = HTML.classList.contains("vm-total-dark");
        const pref = getPreference();
        const mode = HTML.getAttribute(MODE_ATTR) || "off";
        toggleButton.textContent = enabled ? "" : "";
        toggleButton.title = enabled
            ? `Disable dark mode\nMode: ${mode} (preference: ${pref})\nRight-click to cycle auto/enhance/invert.`
            : "Enable dark mode";
    }

    function applyMode() {
        if (!HTML.classList.contains("vm-total-dark")) {
            HTML.removeAttribute(MODE_ATTR);
            updateButtonLabel();
            return;
        }
        const mode = detectMode();
        HTML.setAttribute(MODE_ATTR, mode);
        updateButtonLabel();
    }

    function queueRecalc() {
        if (recalcQueued) return;
        recalcQueued = true;
        if (recalcTimer) clearTimeout(recalcTimer);
        recalcTimer = setTimeout(() => {
            recalcTimer = 0;
            requestAnimationFrame(() => {
                recalcQueued = false;
                applyMode();
                ensureToggle();
            });
        }, 80);
    }

    function setEnabled(enabled) {
        HTML.classList.toggle("vm-total-dark", enabled);
        safeSetValue(STORAGE_KEY, enabled);
        applyMode();
    }

    function cyclePreference() {
        const current = getPreference();
        const idx = MODE_SEQUENCE.indexOf(current);
        const next = MODE_SEQUENCE[(idx + 1) % MODE_SEQUENCE.length];
        cachedPreference = next;
        safeSetValue(HOST_PREF_KEY, next);
        applyMode();
    }

    function ensureToggle() {
        if (!document.body || document.getElementById("vm-total-dark-toggle")) return;
        toggleButton = document.createElement("button");
        toggleButton.id = "vm-total-dark-toggle";
        toggleButton.type = "button";
        toggleButton.setAttribute("aria-label", "Toggle Total Dark Mode");
        toggleButton.addEventListener("click", () => {
            const enabled = HTML.classList.contains("vm-total-dark");
            setEnabled(!enabled);
        });
        toggleButton.addEventListener("contextmenu", (event) => {
            event.preventDefault();
            cyclePreference();
        });
        document.body.appendChild(toggleButton);
        updateButtonLabel();
    }

    ensureStyle();
    setEnabled(Boolean(safeGetValue(STORAGE_KEY, true)));

    const observer = new MutationObserver(() => queueRecalc());
    observer.observe(document.documentElement, {
        childList: true,
        subtree: true,
        attributes: true,
        attributeFilter: ["class", "style", "data-theme", "data-color-mode"],
    });

    if (document.readyState === "loading") {
        document.addEventListener("DOMContentLoaded", () => {
            applyMode();
            ensureToggle();
        }, { once: true });
    } else {
        applyMode();
        ensureToggle();
    }

    window.addEventListener("resize", queueRecalc);
    window.addEventListener("load", () => { applyMode(); ensureToggle(); }, { once: true });
    console.log(" Total Dark Mode loaded (Violentmonkey v8.0)");
