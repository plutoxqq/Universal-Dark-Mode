    }

    GM_addStyle(buildCss());

    function applyMode() {
        if (!HTML.classList.contains('vm-total-dark')) {
            HTML.removeAttribute(MODE_ATTR);
            refreshButton();
            return;
        }
        HTML.setAttribute(MODE_ATTR, guessMode());
        refreshButton();
    }

    function refreshButton() {
        if (!buttonEl) return;
        const enabled = HTML.classList.contains('vm-total-dark');
        const pref = safeGet(modeStorageKey(), 'auto');
        const mode = HTML.getAttribute(MODE_ATTR) || 'off';
        buttonEl.textContent = enabled ? '' : '';
        buttonEl.title = enabled
            ? `Disable dark mode\nMode: ${mode} (preference: ${pref})\nRight-click to cycle auto/enhance/invert for this site.`
            : 'Enable dark mode';
    }

    function scheduleRecalc() {
        if (scheduledRecalc) return;
        scheduledRecalc = true;
        requestAnimationFrame(() => {
            scheduledRecalc = false;
            applyMode();
            ensureButton();
        });
    }

    function setEnabled(enabled) {
        HTML.classList.toggle('vm-total-dark', enabled);
        safeSet(STORAGE_KEY, enabled);
        applyMode();
    }

    function cycleModePreference() {
        const current = safeGet(modeStorageKey(), 'auto');
        const idx = MODE_SEQUENCE.indexOf(current);
        const next = MODE_SEQUENCE[(idx + 1) % MODE_SEQUENCE.length];
        safeSet(modeStorageKey(), next);
        applyMode();
    }

    function ensureButton() {
        if (!document.body || document.getElementById('vm-total-dark-toggle')) return;
        buttonEl = document.createElement('button');
        buttonEl.id = 'vm-total-dark-toggle';
        buttonEl.type = 'button';
        buttonEl.setAttribute('aria-label', 'Toggle total dark mode');
        buttonEl.addEventListener('click', () => setEnabled(!HTML.classList.contains('vm-total-dark')));
        buttonEl.addEventListener('contextmenu', (event) => { event.preventDefault(); cycleModePreference(); });
        document.body.appendChild(buttonEl);
        refreshButton();
    }

    setEnabled(Boolean(safeGet(STORAGE_KEY, true)));
    const observer = new MutationObserver(() => scheduleRecalc());
    observer.observe(document.documentElement, { childList: true, subtree: true, attributes: true, attributeFilter: ['class', 'style'] });
    if (document.readyState === 'loading') {
        document.addEventListener('DOMContentLoaded', () => { applyMode(); ensureButton(); }, { once: true });
    } else {
        applyMode();
        ensureButton();
    }
    window.addEventListener('load', applyMode, { once: true });
    window.addEventListener('resize', scheduleRecalc);
    console.log(' Total Dark Mode loaded (Violentmonkey v7.0)');
})();
