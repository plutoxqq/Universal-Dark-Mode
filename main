    function safeGet(storageKey, fallbackValue) {
        try {
            if (typeof GM_getValue === 'function') {
                return GM_getValue(storageKey, fallbackValue);
            }
        } catch (_) {}
        try {
            const raw = localStorage.getItem(storageKey);
            if (raw == null) {
                return fallbackValue;
            }
            if (raw === 'true') return true;
            if (raw === 'false') return false;
            return raw;
        } catch (_) {
            return fallbackValue;
        }
    }

    function safeSet(storageKey, value) {
        try {
            if (typeof GM_setValue === 'function') {
                GM_setValue(storageKey, value);
                return;
            }
        } catch (_) {}
        try {
            localStorage.setItem(storageKey, String(value));
        } catch (_) {}
    }

    function modeStorageKey() {
        return `${MODE_PREF_PREFIX}${location.hostname}`;
    }

    function parseRgbTriplet(colorText) {
        if (!colorText) return null;
        const m = colorText.match(/rgba?\(([^)]+)\)/i);
        if (!m) return null;
        const values = m[1].split(',').map((v) => Number.parseFloat(v.trim()));
        if (values.length < 3 || values.slice(0,3).some((v) => Number.isNaN(v))) return null;
        return [values[0], values[1], values[2]];
    }

    function relativeLuminance(rgb) {
        const normalized = rgb.map((v) => v / 255).map((c) => (c <= 0.03928 ? c / 12.92 : ((c + 0.055) / 1.055) ** 2.4));
        return (0.2126 * normalized[0]) + (0.7152 * normalized[1]) + (0.0722 * normalized[2]);
    }

    function contrastRatio(l1, l2) {
        const light = Math.max(l1, l2);
        const dark = Math.min(l1, l2);
        return (light + 0.05) / (dark + 0.05);
    }

    function effectiveColor(element, propertyName) {
        let node = element;
        while (node && node.nodeType === Node.ELEMENT_NODE) {
            const value = getComputedStyle(node)[propertyName];
            const rgb = parseRgbTriplet(value);
            if (rgb) {
                const alphaMatch = value.match(/rgba\([^,]+,[^,]+,[^,]+,\s*([\d.]+)\)/i);
                const alpha = alphaMatch ? Number.parseFloat(alphaMatch[1]) : 1;
                if (!Number.isNaN(alpha) && alpha > 0.03) {
                    return rgb;
                }
            }
            node = node.parentElement;
        }
        return null;
    }

    function sampleDocumentTone() {
        const w = Math.max(window.innerWidth || 1, 1);
        const h = Math.max(window.innerHeight || 1, 1);
        const points = [
            [0.5, 0.5],
            [0.15, 0.15],
            [0.85, 0.15],
            [0.15, 0.85],
            [0.85, 0.85],
            [0.5, 0.2],
            [0.5, 0.8],
            [0.2, 0.5],
            [0.8, 0.5],
            [0.35, 0.35],
            [0.65, 0.35],
            [0.35, 0.65],
            [0.65, 0.65],
        ];
        let darkness = 0;
        let lightness = 0;
        let votes = 0;
        for (const [xr, yr] of points) {
            const x = Math.max(0, Math.min(w - 1, Math.floor(w * xr)));
            const y = Math.max(0, Math.min(h - 1, Math.floor(h * yr)));
            const el = document.elementFromPoint(x, y);
            if (!el) continue;
            const bg = effectiveColor(el, 'backgroundColor') || parseRgbTriplet(getComputedStyle(HTML).backgroundColor);
            const fg = effectiveColor(el, 'color') || parseRgbTriplet(getComputedStyle(el).color);
            if (!bg || !fg) continue;
            const bgLum = relativeLuminance(bg);
            const fgLum = relativeLuminance(fg);
            const contrast = contrastRatio(bgLum, fgLum);
            votes += 1;
            if (bgLum < 0.42 && fgLum > bgLum && contrast >= 2.2) darkness += 1;
            if (bgLum > 0.62 && contrast >= 2.2) lightness += 1;
        }
        if (!votes) return null;
        return { darkRatio: darkness / votes, lightRatio: lightness / votes, votes };
    }

    function hasDarkSignals() {
        const metaColorScheme = document.querySelector('meta[name="color-scheme"]');
        if (metaColorScheme) {
            const c = String(metaColorScheme.content || '').toLowerCase();
            if (c.includes('dark') && !c.includes('only light')) return true;
        }
        const themeMeta = document.querySelector('meta[name="theme-color"]');
        if (themeMeta) {
            const rgb = parseRgbTriplet(themeMeta.content || '');
            if (rgb && relativeLuminance(rgb) < 0.45) return true;
        }
        const htmlScheme = (getComputedStyle(HTML).colorScheme || '').toLowerCase();
        const bodyScheme = document.body ? (getComputedStyle(document.body).colorScheme || '').toLowerCase() : '';
        if (htmlScheme.includes('dark') || bodyScheme.includes('dark')) return true;
        return false;
    }

    function guessMode() {
        const pref = safeGet(modeStorageKey(), 'auto');
        if (pref === 'enhance' || pref === 'invert') return pref;
        const host = location.hostname.toLowerCase();
        if (KNOWN_SITE_MODE_HINTS.has(host)) return KNOWN_SITE_MODE_HINTS.get(host);
        if (hasDarkSignals()) return 'enhance';
        const sample = sampleDocumentTone();
        if (sample) {
            if (sample.darkRatio >= 0.55) return 'enhance';
            if (sample.lightRatio >= 0.55) return 'invert';
            if (sample.darkRatio >= sample.lightRatio) return 'enhance';
            return 'invert';
        }
        const htmlBg = parseRgbTriplet(getComputedStyle(HTML).backgroundColor);
        const bodyBg = document.body ? parseRgbTriplet(getComputedStyle(document.body).backgroundColor) : null;
        const bg = bodyBg || htmlBg;
        if (!bg) return 'enhance';
        return relativeLuminance(bg) < 0.48 ? 'enhance' : 'invert';
    }

    function cssForSelectors(selectors, body) {
        return selectors.map((selector) => `${selector} { ${body} }`).join('\n');
    }

    function buildCss() {
        const enhanceSurface = cssForSelectors(SURFACE_SELECTORS, `background-color: ${DARK_BG} !important; color: ${LIGHT_TEXT} !important; border-color: #2d313a !important;`);
        const enhanceInteractive = cssForSelectors(INTERACTIVE_SELECTORS, `background-color: #1a1c22 !important; color: ${LIGHT_TEXT} !important; border-color: #39414d !important;`);
        const reinvertMedia = MEDIA_REINVERT_SELECTORS.join(',\n        ');
        return `
html.vm-total-dark {
    color-scheme: dark !important;
    scrollbar-color: #4a4a4a #111 !important;
}
html.vm-total-dark body {
    background: transparent !important;
}
html.vm-total-dark a { color: ${ACCENT_LINK} !important; }
html.vm-total-dark a:visited { color: ${ACCENT_VISITED} !important; }
html.vm-total-dark a:hover, html.vm-total-dark a:focus { color: ${ACCENT_HOVER} !important; }
html.vm-total-dark input, html.vm-total-dark textarea, html.vm-total-dark select, html.vm-total-dark button, html.vm-total-dark [role="button"] {
    color-scheme: dark !important;
}
